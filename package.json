{
  "name": "rute",
  "version": "0.1.0",
  "description": "Allows you to require stuff from your project root",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha",
    "codeclimate": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/.bin/codeclimate && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ksmithut/rute.git"
  },
  "keywords": [
    "project",
    "require"
  ],
  "author": "ksmithut",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ksmithut/rute/issues"
  },
  "homepage": "https://github.com/ksmithut/rute",
  "devDependencies": {
    "codeclimate-test-reporter": "0.0.3",
    "expect.js": "^0.3.1",
    "istanbul": "^0.3.0",
    "mocha": "^1.21.4",
    "mocha-lcov-reporter": "0.0.1"
  }
}
